{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-merge-modules"],
  "jsDocCompatibility": {
    "exampleTag": true,
    "defaultTag": true
  },
  "objectLiteralTypeDeclarationStyle": "list",
  "entryPoints": [
    "./src/index.ts",
    "./src/angular/index.ts",
    "./src/react/index.ts",
    "./src/lit/index.ts",
    "./src/assertable/index.ts"
  ],
  "out": "documents",
  "disableSources": true,
  "githubPages": false
}
