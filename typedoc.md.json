{
"$schema": "https://typedoc.org/schema.json",
"plugin": ["typedoc-plugin-missing-exports", "typedoc-plugin-markdown"],
  "jsDocCompatibility": {
      "exampleTag": true,
      "defaultTag": true
  },
  "objectLiteralTypeDeclarationStyle": "list",
  "entryPoints": ["./src/index.ts", "./src/angular/index.ts", "./src/react/index.ts", "./src/lit/index.ts" ],
  "out": "docs",
}