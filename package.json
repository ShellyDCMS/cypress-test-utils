{
  "name": "@shellygo/cypress-test-utils",
  "description": "Test Automation Utilities",
  "version": "1.0.8",
  "author": "Shelly Goldblit",
  "private": false,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf dist/* && tsc && mv dist/out-tsc/src/lit dist/out-tsc/lit  && mv dist/out-tsc/src/angular dist/out-tsc/angular && mv dist/out-tsc/src/react dist/out-tsc/react",
    "package": "node --max-old-space-size=4400 && npm run build",
    "publish-package": "cd dist/out-tsc && npm publish",
    "generate-docs": "npx typedoc  --includeVersion --plugin typedoc-plugin-markdown --out /docs src/index.ts src/angular/index.ts src/lit/index.ts src/react/index.ts"
  },
  "dependencies": {
    "@cypress/mount-utils": "^4.0.0",
    "@types/chai-subset": "^1.3.3",
    "chai-subset": "^1.6.0",
    "cypress": "^12.5.1",
    "cypress-real-events": "^1.7.6",
    "cypress-wait-if-happens": "^1.3.3",
    "cypress-wait-until": "^1.7.2"
  },
  "devDependencies": {
    "@angular/core": "^14.2.0",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "cpx": "^1.5.0",
    "cross-var": "^1.1.0",
    "gh-pages": "^5.0.0",
    "lit": "^2.7.4",
    "react": "^18.2.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^4.6.4"
  },
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist/out-tsc",
    "strict": true
  },
  "main": "./src/index.js",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./angular": {
      "types": "./angular/index.d.ts",
      "default": "./angular/index.js"
    },
    "./lit": {
      "types": "./lit/index.d.ts",
      "default": "./lit/index.js"
    }
  },
  "type": "module",
  "include": [
    "src"
  ],
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=14.18.0"
  }
}
